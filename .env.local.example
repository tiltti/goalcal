# Local Development Environment
# Copy this to .env.local

# DynamoDB Local endpoint (when using Docker)
DYNAMODB_ENDPOINT=http://localhost:8000

# AWS Region
AWS_REGION=eu-north-1

# Local AWS credentials (for DynamoDB Local)
AWS_ACCESS_KEY_ID=local
AWS_SECRET_ACCESS_KEY=local

# Session secret for cookie signing (change in production!)
SESSION_SECRET=your-secret-key-here-min-32-chars

# Admin password for /admin page
ADMIN_PASSWORD=admin123

# DynamoDB table name
DYNAMODB_TABLE=goalcal
